{
	"image": "dietznuts/wine-and-xpra:latest",
	"containerEnv": {
		"XDG_RUNTIME_DIR": "/tmp"
	},
	"containerUser": "penguin",
	"remoteUser": "penguin",
	"postCreateCommand": "wineboot --init",
	"postStartCommand": "wineserver -w && xpra start --bind-tcp=0.0.0.0:1337 --html=on --daemon=no --exit-with-children --start-child=winefile",
	"forwardPorts": [1337],
	"portsAttributes": {
		"1337": {
			"label": "File Explorer",
			"onAutoForward": "openBrowser"
		}
	}
}
